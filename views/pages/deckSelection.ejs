<!DOCTYPE html>
<html lang = "en">
<head>
	<meta charset = "utf-8"/>
	<title><%=user.name%>'s Decks</title>
	<link rel="stylesheet" href="/stylesheets/main.css"/>
</head>

<body>

    <%- include ("../partials/aio_dld_header.ejs") %>

    <ul>
        <% decks.forEach(function(deck) { %>
            <li><a href="/deckBuild/edit/<%= deck.id %>"><%= deck.name %></a></li>
        <% }); %>
    </ul>

    <button onclick="goToPage('/deckBuild/build')">New Deck</button>
    
</body>